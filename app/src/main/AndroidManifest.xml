<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.koupner.koupner">

    <application
        android:name=".root.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".home.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".intro.IntroActivity" />
        <activity
            android:name=".restaurantreview.RestaurantReviewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".restaurantmenu.RestaurantMenuActivity" />
        <activity android:name=".restaurantmenu.ProductDetail" />
        <activity android:name=".cart.CartActivity"></activity>
    </application>

</manifest>